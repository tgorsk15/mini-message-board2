<html>
    <head>
        <title>HomePage</title>
    </head>
    <body>
        <% for (let i = 0; i < messages.length; i++) { %>
            <h4 class="message-user">
                <%= messages[i].user %>
            </h4>
            <div class="content-container">
                <p class="message-content">
                    <%= messages[i].text %>
                </p>
                <p class="message-time">
                    at <%= messages[i].added %>
                </p>
            </div>
            
        <% } %>
    </body>
</html>